{
  "name": "acquia/acquia-ra-composer",
  "description": "Project json for Acquia Remote Administration Composer builds",
  "type": "project",
  "license": "proprietary",
  "require": {
    "drupal/config_installer": "^1.8",
    "composer/installers": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "drupal/config_installer": "^1.8",
    "drupal/console": "~1.0",
    "drupal/core": "^8.6",
    "drush/drush": "^8.1",
    "drupal/at_tools": "^3.2",
    "drupal/ctools": "^3.0",
    "drupal/devel": "^1.2",
    "drupal/pathauto": "^1.3",
    "drupal/token": "^1.5",
    "drupal/devel_generate": "^1.2",
    "drupal/kint": "^1.2",
    "drupal/video_embed_media": "^2.0",
    "drupal/corolla": "^3.0",
    "drupal/pixture_reloaded": "^3.0",
    "drupal/video_embed_field": "^2.0",
    "d3/d3": "^3.5",
    "drupal/backup_migrate": "^4.0",
    "drupal/login_popup": "^1.0",
    "drupal/commerce": "^2.9",
    "drupal/paragraphs": "^1.3",
    "drupal/user_registrationpassword": "^1.0",
    "drupal/bat": "^1.0",
    "drupal/bee": "^1.0",
    "drupal/mailchimp": "^1.7",
    "drupal/webform_mailchimp": "^5.4",
    "drupal/admin_toolbar": "^1.2",
    "drupal/entity_usage": "^2.0",
    "drupal/entity_browser": "^2.0",
    "drupal/rules": "^3.0",
    "drupal/flag": "^4.0",
    "drupal/webform": "^5.0",
    "drupal/facets": "^1.1",
    "drupal/search_api": "^1.1",
    "components/highlightjs": "^9.7",
    "wikimedia/composer-merge-plugin": "^1.4",
    "jquery/chosen": "1.8.7",
    "bower-asset/jquery-ui-slider-pips": "^1.11",
    "drupal/clientside_validation": "^1.0",
    "drupal/antibot": "^1.2",
    "drupal/honeypot": "^1.2",
    "drupal/captcha": "^1.0",
    "drupal/imce": "^1.7",
    "drupal/webform_views": "^5.0",
    "drupal/webform_composite": "^1.0",
    "minnur/webform_checkboxes_table": "1.0",
    "drupal/webform_node_element": "^1.0",
    "drupal/webform_shs": "^1.0",
    "drupal/webform_wizard_full_title": "^1.0",
    "drupal/webform_layout_container": "^1.0",
    "drupal/entity_reference_revisions": "^1.6",
    "drupal/ui_patterns": "^1.0",
    "drupal/paragraphs_jquery_ui_accordion": "^1.2",
    "drupal/insert_view": "^1.0",
    "drupal/simplenews_scheduler": "^1.0",
    "drupal/simplenews": "^1.0",
    "drupal/swiftmailer": "^1.0",
    "chx/webform_product": "1.0",
    "drupal/webform_simplenews_handler": "^1.0",
    "drupal/mailsystem": "^4.1",
    "drupal/webform_mass_email": "^1.0",
    "drupal/smtp": "^1.0",
    "drupal/webform_analysis": "^1.0",
    "drupal/webform_invitation": "^1.0",
    "drupal/webform_permissions_by_term": "^1.0",
    "drupal/webform_sanitize": "^1.0",
    "drupal/webform_scheduled_tasks": "^1.0",
    "drupal/webform_encrypt": "^1.0",
    "drupal/webform_ip_track": "^1.0",
    "drupal/payment": "^2.0"

  },
  
  "require-dev": {
    "drupal/devel": "^1.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/twig_xdebug": "^1.0",
    "drupal/twig_tweak": "^2.1"
 
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "installer-paths": {
    "libraries/{$name}":["type:drupal-library"]

  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type":"package",
      "package": {
        "name":"d3/d3",
        "version": "3.5.17",
        "type": "drupal-library",
        "dist": {
          "url":"https://github.com/d3/d3/archive/v3.5.17.zip",
          "type":"zip"
        }
      }
    },
    {
            "type": "package",
            "package": {
                "name": "jquery/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery.chosen"
                },
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
    },
    {
            "type": "package",
            "package": {
                "name": "minnur/webform_checkboxes_table",
                "version": "1.0",
                "type": "drupal-module",
                "extra": {
                    "installer-name": "webform_checkboxes_table"
                },
                "dist": {
                    "url": "https://github.com/minnur/webform_checkboxes_table/archive/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
    },
    {
            "type": "package",
            "package": {
                "name": "chx/webform_product",
                "version": "1.0",
                "type": "drupal-module",
                "extra": {
                    "installer-name": "webform_product"
                },
                "dist": {
                    "url": "https://github.com/chx/webform_product/archive/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
    },
    {
      "type":"package",
      "package":{
        "name":"components/highlights",
        "version":"9.7.0",
        "type":"drupal-library",
        "dist": {
          "url":"https://github.com/components/highlightjs/archive/9.7.0.zip",
          "type":"zip"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "merge-plugin": {
            "include": [
                "docroot/modules/contrib/webform/composer.libraries.json"
            ]
        }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "post-update-cmd": [
      "@composer drupal-scaffold"
    ],
    "drupal-scaffold": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "enable-patching": true,
  "patches": {
    "drupal/[module_name]": {
      "Note regarding the nature of the patch being applied": "https://www.drupal.org/files/issues/[patch_name].patch"
    }
  }
}
